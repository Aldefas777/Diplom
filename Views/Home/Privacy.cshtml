@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<form method ="post">
    <div>
        <div class="form-group">
           <label>Id</label>
           <input name="Id" type="text" size="50" value="Name" />
       </div>
       <div class="form-group">
           <label>Имя</label>
           <input name="Names" type="text" size="50" value="Name" />
       </div>
       <div class="form-group">
           <label>Фамилия</label>
           <input name="Surname" type="text" size="50" value="Name" />
       </div>
       <div class="form-group">
           <label>Отчество</label>
           <input name="SecondName" type="text" size="50" value="Name" />
       </div>
       <div class="form-group">
           <label>Абониметн</label>
           <input name="Aboniment" type="text" size="50" value="Name" />
       </div>
       <input type="submit" value="Update" class="submit" />
    </div>
</form>
